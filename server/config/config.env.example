# Backend Environment Variables
# Copy this file to config.env and fill in your values

# Server Configuration
NODE_ENV=development
PORT=5001

# MongoDB Connection
# For local MongoDB: mongodb://localhost:27017/clinic-management
# For MongoDB Atlas: mongodb+srv://<username>:<password>@cluster.mongodb.net/clinic-management
MONGO_URI=mongodb://localhost:27017/clinic-management

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRE=30d

# Frontend URL (for CORS)
# For local development: http://localhost:5173
# For production: https://your-frontend-url.vercel.app
FRONTEND_URL=http://localhost:5173